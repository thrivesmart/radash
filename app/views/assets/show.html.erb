<% content_for :page_title do %>
asset Details - 
<% end %>
<div class="container">
<div class="page-header">
<h1>
<div class="pull-left">
<%= link_to "<span class=\"glyphicon glyphicon-step-backward\" aria-hidden=\"true\"></span>".html_safe, assets_path, class: "btn btn-default", title: "Back to assets" %>
&nbsp;
</div>
<div class="pull-right">
<%= link_to "<span class=\"glyphicon glyphicon-pencil\" aria-hidden=\"true\"></span> Edit".html_safe, edit_asset_path(@asset), class: "btn btn-warning" %>
</div>
asset Details
</h1>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Org</label>
  <div class="col-sm-9"><%= @asset.org %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">File</label>
  <div class="col-sm-9"><%= @asset.file %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Html</label>
  <div class="col-sm-9"><%= @asset.html %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Width</label>
  <div class="col-sm-9"><%= @asset.width %></div>
</div>

<div class="row">
  <label class="col-sm-3 text-right text-muted">Height</label>
  <div class="col-sm-9"><%= @asset.height %></div>
</div>

</div>

